<?xml version="1.0" ?>
<sdf version="1.8">
  <model name='multi_joint_robot'>
    <link name="link1"/>
    <link name="link2"/>
    <link name="link3"/>

    <joint name='j12' type='revolute2'>
        <parent>link1</parent>
        <child>link2</child>
        <axis>
          <xyz>0 0 1</xyz>
          <limit>
            <lower>-1</lower>
            <upper>1</upper>
          </limit>
        </axis>
    </joint>

    <joint name='j23' type='revolute2'>
        <parent>link2</parent>
        <child>link3</child>
        <axis>
          <xyz>0 0 1</xyz>
          <limit>
            <lower>-1</lower>
            <upper>1</upper>
          </limit>
        </axis>
    </joint>

  </model>
</sdf>